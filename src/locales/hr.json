    "diets": {
        "vegan": {
            "name": "Veganska",
            "description": "Veganska prehrana isključuje sve životinjske proizvode, uključujući meso, mlijeko i jaja. Ova prehrana se fokusira na biljnu hranu poput povrća, voća, orašastih plodova, sjemenki i mahunarki.",
            "url": "https://www.healthline.com/nutrition/vegan-diet-guide#about-the-vegan-diet"
        },
        "vegetarian": {
            "name": "Vegetarijanska",
            "description": "Vegetarijanska prehrana uključuje povrće, voće, jaja i mliječne proizvode, ali isključuje meso i ribu. Može se prilagoditi za zadovoljavanje prehrambenih potreba uz uravnoteženo planiranje obroka.",
            "url": "https://www.mayoclinic.org/healthy-lifestyle/nutrition-and-healthy-eating/in-depth/vegetarian-diet/art-20046446"
        },
        "ketogenic": {
            "name": "Ketogena",
            "description": "Ketogena prehrana je prehrana s visokim udjelom masti i niskim udjelom ugljikohidrata koja ima za cilj inducirati stanje ketoze u tijelu, gdje se mast koristi za energiju umjesto ugljikohidrata.",
            "url": "https://www.healthline.com/nutrition/ketogenic-diet-101#diet-types"
        },
        "paleo": {
            "name": "Paleo",
            "description": "Paleo prehrana naglašava konzumaciju cijele hrane koja je vjerojatno bila dostupna našim prapovijesnim precima, fokusirajući se na nemasno meso, ribu, voće, povrće, orašaste plodove i sjemenke.",
            "url": "https://www.mayoclinic.org/healthy-lifestyle/nutrition-and-healthy-eating/in-depth/paleo-diet/art-20111182"
        },
        "gluten_free": {
            "name": "Bez glutena",
            "description": "Prehrana bez glutena uključuje isključivanje hrane koja sadrži protein gluten, uključujući pšenicu, raž i ječam. To je ključno za upravljanje simptomima celijakije i drugih medicinskih stanja povezanih s glutenom.",
            "url": "https://www.mayoclinic.org/healthy-lifestyle/nutrition-and-healthy-eating/in-depth/gluten-free-diet/art-20048530"
        },
        "mediterranean": {
            "name": "Mediteranska",
            "description": "Mediteranska prehrana se uglavnom temelji na prehrambenim navikama južnoeuropskih zemalja. Uključuje mnogo voća, povrća, žitarica, maslinova ulja i svježe ribe. Povezana je s dobrim zdravljem srca i nižim rizikom od nekoliko bolesti.",
            "url": "https://www.healthline.com/nutrition/mediterranean-diet-meal-plan#what-is-it"
        },
        "low_carb": {
            "name": "Niskougljikohidratna",
            "description": "Prehrana s niskim udjelom ugljikohidrata fokusira se na smanjenje ugljikohidrata kako bi se pomoglo u gubitku težine ili upravljanju određenim zdravstvenim stanjima. Hrana bogata ugljikohidratima je ograničena ili zamijenjena hranom koja sadrži veći postotak masti i umjerene proteine.",
            "url": "https://www.healthline.com/nutrition/low-carb-diet-meal-plan-and-menu"
        },
        "dairy_free": {
            "name": "Bez mliječnih proizvoda",
            "description": "Prehrana bez mliječnih proizvoda isključuje sve mliječne proizvode. To može biti potrebno za osobe s intolerancijom na laktozu, alergijama na proteine mliječnih proizvoda ili one koji slijede vegansku prehranu.",
            "url": "https://www.verywellhealth.com/dairy-free-diet-5216769"
        }
    },
