{
    "diets": {
        "vegan": {
            "name": "Vegan",
            "description": "A vegan diet excludes all animal products, including meats, dairy, and eggs. This diet focuses on plant-based foods such as vegetables, fruits, nuts, seeds, and legumes.",
            "url": "https://www.healthline.com/nutrition/vegan-diet-guide#about-the-vegan-diet"
        },
        "vegetarian": {
            "name": "Vegetarian",
            "description": "The vegetarian diet includes vegetables, fruits, eggs, and dairy but excludes meat and fish. It can be adapted to meet nutritional needs with balanced meal planning.",
            "url": "https://www.mayoclinic.org/healthy-lifestyle/nutrition-and-healthy-eating/in-depth/vegetarian-diet/art-20046446"
        },
        "ketogenic": {
            "name": "Ketogenic",
            "description": "The ketogenic diet is a high-fat, low-carbohydrate diet that aims to induce a state of ketosis in the body, where fat is burned for energy instead of carbohydrates.",
            "url": "https://www.healthline.com/nutrition/ketogenic-diet-101#diet-types"
        },
        "paleo": {
            "name": "Paleo",
            "description": "The Paleo diet emphasizes eating whole foods that were likely available to our prehistoric ancestors, focusing on lean meats, fish, fruits, vegetables, nuts, and seeds.",
            "url": "https://www.mayoclinic.org/healthy-lifestyle/nutrition-and-healthy-eating/in-depth/paleo-diet/art-20111182"
        },
        "gluten_free": {
            "name": "Gluten-Free",
            "description": "A gluten-free diet involves excluding foods that contain the protein gluten, including wheat, rye, and barley. It is essential for managing symptoms of celiac disease and other medical conditions associated with gluten.",
            "url": "https://www.mayoclinic.org/healthy-lifestyle/nutrition-and-healthy-eating/in-depth/gluten-free-diet/art-20048530"
        },
        "mediterranean": {
            "name": "Mediterranean",
            "description": "The Mediterranean diet is largely based on the eating habits of southern European countries. It includes a lot of fruits, vegetables, grains, olive oil, and fresh fish. It has been linked to good heart health and a lower risk of several diseases.",
            "url": "https://www.healthline.com/nutrition/mediterranean-diet-meal-plan#what-is-it"
        },
        "low_carb": {
            "name": "Low-Carb",
            "description": "A low-carb diet focuses on reducing carbohydrates to help lose weight or manage certain health conditions. Foods high in carbohydrates are limited or replaced with foods containing a higher percentage of fats and moderate protein.",
            "url": "https://www.healthline.com/nutrition/low-carb-diet-meal-plan-and-menu"
        },
        "dairy_free": {
            "name": "Dairy-Free",
            "description": "The dairy-free diet excludes all dairy products. This can be necessary for those with lactose intolerance, dairy protein allergies, or those following a vegan diet.",
            "url": "https://www.verywellhealth.com/dairy-free-diet-5216769"
        }
    },
